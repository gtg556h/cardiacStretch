import numpy as np
import pdb
import matplotlib.pyplot as plt




class experiment(object):

    def __init__(self, cellEvents, substrateEvents, params):

        # Recover parameters:
        
        self.dt = params['dt']

        # Preprocessing:
        self.c_theta = self.phaseGen(cellEvents)
        self.s_theta = self.phaseGen(substrateEvents)
        self.clipTime()
        self.relativePhase()







